<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>Keswa_Project</RootNamespace>
	<AssemblyName>Keswa_Project</AssemblyName>
	  <RuntimeIdentifier>win-x86</RuntimeIdentifier>

  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Kesawa Data Access\**" />
    <Compile Remove="Keswa Entities\**" />
    <Compile Remove="Keswa Untilities\**" />
    <Content Remove="Kesawa Data Access\**" />
    <Content Remove="Keswa Entities\**" />
    <Content Remove="Keswa Untilities\**" />
    <EmbeddedResource Remove="Kesawa Data Access\**" />
    <EmbeddedResource Remove="Keswa Entities\**" />
    <EmbeddedResource Remove="Keswa Untilities\**" />
    <None Remove="Kesawa Data Access\**" />
    <None Remove="Keswa Entities\**" />
    <None Remove="Keswa Untilities\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Mapster" Version="7.4.0" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="8.0.19" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR" Version="1.2.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="8.0.17">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Stripe.net" Version="48.5.0-beta.1" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.6.2" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\Images\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="Keswa Entities\Keswa Entities.csproj" />
    <ProjectReference Include="Keswa Untilities\Keswa Untilities.csproj" />
  </ItemGroup>

</Project>
